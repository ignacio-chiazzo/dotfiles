# Aliases

alias rbmaster="git checkout master & git pull origin master & git checkout - & git rebase master"
alias test='bin/test --include-branch-commits'
alias style='bin/style --include-branch-commits'
alias check='bin/style --include-branch-commits;bin/srb typecheck -a'
alias logs='tail -f log/development.log'
alias up='bundle install;bin/rails db:migrate'


# To open cursor with `cursor .` (Assuming the Cursor application is in the /Applications folder)
 alias cursor="/Applications/Cursor.app/Contents/MacOS/cursor"
